# Backend Environment Variables Template
# Copy this file to .env and fill in your actual values

# OpenAI API Key (get from https://platform.openai.com/account/api-keys)
# For local development without OpenAI, set ALLOW_NO_OPENAI=1 instead
OPENAI_API_KEY=sk-proj-your-key-here

# Server port
PORT=3001

# Development mode: allows backend to run without valid OpenAI key
# Set to 1 for local testing, 0 for production
ALLOW_NO_OPENAI=0
